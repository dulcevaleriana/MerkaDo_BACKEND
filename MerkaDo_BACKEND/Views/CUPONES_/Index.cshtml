@model IEnumerable<MerkaDo_BACKEND.Models.CUPONES_>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreCupones)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descuentoCupones)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.precioDescontar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaVencimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.codigoCupones)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreCupones)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descuentoCupones)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.precioDescontar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaVencimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.codigoCupones)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.cuponesId }) |
            @Html.ActionLink("Details", "Details", new { id=item.cuponesId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.cuponesId })
        </td>
    </tr>
}

</table>
